select
{
	display: inline-block;
	margin-bottom: -6px;
	height: 20px;
	font-effect: shadow(1px 1px black);
}

select selectvalue
{
	margin-right: 20dp;
	padding: 3px 0px 0px 4px;
	decorator: ninepatch( combo-active-bg, combo-active-bg-inner );
}
select:focus-visible selectvalue {
	decorator: image( selectvalue-hover );
}

select selectarrow
{
	width: 20dp;
	height: 20dp;

	decorator: image( combo-down-arrow fill );
}

select:hover selectarrow, select:focus-visible selectarrow
{
	decorator: image( combo-down-arrow fill );
}

select:active selectarrow,
select selectarrow:checked
{
	decorator: image( combo-up-arrow );
}

select selectbox
{
	padding: 1dp 4dp 4dp 4dp;
	margin: 0px 0 0 -4px;
}

select selectbox,
tbody
{
	max-height: 250px;
}

select selectbox option
{
	width: auto;
	padding: 3dp 0 3dp 6dp;
	decorator: ninepatch( combo-bg, combo-bg-inner );
}
select selectbox option:checked
{
	decorator: ninepatch( combo-active-bg, combo-active-bg-inner );
}
select selectbox option:hover
{
	decorator: ninepatch( combo-active-bg, combo-active-bg-inner );
}

textarea {
	display: inline-block;
	text-align: left;
	line-height: 1.3;
	color: white;
}

input[type="text"], input[type="password"] {
	decorator: ninepatch(input-bg, input-bg-inner);
	font-effect: shadow(1px 1px black);
	color: #f3be5d;
	display: inline-block;
	padding: 6px;
}

input[type="text"]:disabled, input[type="password"]:disabled {
	font-effect: shadow(1px 1px black);
	color: #999;
}