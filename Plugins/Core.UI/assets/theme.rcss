.panel {
	decorator: ninepatch(panel, panel-inner);
}

/* Force the tabset element to a fixed size. */
tabset
{
	display: block;
}

/* Display the tab container as a block element 20 pixels high; it will
   be positioned at the top of the tabset. */
tabset tabs
{
	display: block;
	decorator: image( tab-header-bg );
	padding: 4dp 4dp 0 4dp;
}

/* Force each tab to only take up 50 pixels across the tabs element. */
tabset tab
{
	display: inline-block;
	decorator: ninepatch(tab, tab-inner);
	padding: 6px 12px;
	cursor: 0x06004d69;
}

/* Force each tab to only take up 50 pixels across the tabs element. */
tabset tab:hover
{
	decorator: ninepatch(tab-hovered, tab-hovered-inner);
}
tabset tab:active
{
	decorator: ninepatch(tab-active, tab-active-inner);
}
tabset tab:selected
{
	decorator: ninepatch(tab-selected, tab-selected-inner);
}

/* Display the panel container as a block element 180 pixels high; it will
   be positioned below the tab container and take up the rest of the space
   in the tabset. */
tabset panels
{
	display: block;
}

/* Fix each panel to take up exactly the panelled space. */
tabset panels panel
{
	display: block;
	width: 100%;
	height: 100%;
}


scrollbarvertical {
	width: 16px;
}

scrollbarvertical slidertrack {
	decorator: image("scroll-vert-bg.png" repeat-y);
}

scrollbarvertical sliderbar {
	width: 16px;
	min-height: 46px;
	decorator: tiled-vertical( scroll-bar-vert-top, scroll-bar-vert-mid, scroll-bar-vert-bottom );
	cursor: 0x06004d69;
}

scrollbarvertical sliderbar:hover {
	decorator: tiled-vertical( scroll-bar-vert-top-hover, scroll-bar-vert-mid-hover, scroll-bar-vert-bottom-hover );
}

scrollbarvertical sliderbar:active {
	decorator: tiled-vertical( scroll-bar-vert-top-active, scroll-bar-vert-mid-active, scroll-bar-vert-bottom-active );
}

scrollbarvertical sliderarrowdec {
	width: 16px;
	height: 16px;
	decorator: image( scroll-up-button );
	cursor: 0x06004d69;
}

scrollbarvertical sliderarrowdec:hover {
	decorator: image( scroll-up-button-hover );
}

scrollbarvertical sliderarrowdec:active {
	decorator: image(scroll-up-button-active );
}

scrollbarvertical sliderarrowinc {
	width: 16px;
	height: 16px;
	decorator: image( scroll-down-button );
	cursor: 0x06004d69;
}

scrollbarvertical sliderarrowinc:hover {
	decorator: image( scroll-down-button-hover );
}

scrollbarvertical sliderarrowinc:active {
	decorator: image( scroll-down-button-active );
}

button {
	display: inline-block;
	cursor: 0x06004d69;
	padding: 8px;
	margin: 0px 2px;
	text-align: center;
	decorator: ninepatch(button-primary, button-primary-inner);
  click-sound: 0x0A0003B3;
	cursor: 0x06004d69;
}

button:hover {
	decorator: ninepatch(button-primary-hover, button-primary-hover-inner);
}

button:active {
	decorator: ninepatch(button-primary-active, button-primary-active-inner);
}

button[disabled] {
	decorator: ninepatch(button-primary-disabled, button-primary-disabled-inner);
}

button.secondary {
	padding: 6px 8px 6px 8px;
	decorator: ninepatch(button-secondary, button-secondary-inner);
}

button.secondary:hover {
	decorator: ninepatch(button-secondary-hover, button-secondary-hover-inner);
}

button.secondary:active {
	decorator: ninepatch(button-secondary-active, button-secondary-active-inner);
}

button.secondary[disabled] {
	decorator: ninepatch(button-secondary-disabled, button-secondary-disabled-inner);
}

button.small {
	padding: 4px 6px 2px 6px;
	font-effect: shadow(1px 1px black);
	font-size: 12px;
	decorator: ninepatch(button-small, button-small-inner);
}

button.small:hover {
	decorator: ninepatch(button-small-hover, button-small-hover-inner);
}

button.small:active {
	decorator: ninepatch(button-small-active, button-small-active-inner);
}

button.small[disabled] {
	decorator: ninepatch(button-small-disabled, button-small-disabled-inner);
}

