<html>
	<head>
		<style>
          body {
            display: block;
            font-family: LatoLatin;
            height: 600px;
            width: 800px;
            color: #fff;
            opacity: 1;
            font-size: 36px;
          }
          div#wrapper {
            text-align: center;
            margin: auto;
            display: block;
            width: 800px;
            height: 440px;
            decorator: image( dat://0x06006E20 );
          }
        </style>
	</head>
	<body data-model="DatPatchScreen">
        <div id="wrapper">
            Connect: {{ ConnectPercentage * 100 | format(2) }}%
            Patch: {{ PatchPercentage * 100 | format(2) }}%
            <br />
            Bytes: {{ ReceivedBytes }} / {{ ExpectedBytes }}
        </div>
	</body>
</html>