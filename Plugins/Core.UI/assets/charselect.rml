        height: 99px;
        line-height: 99px;
        font-size: 18dp;
        font-effect: outline(2px black);
      }

      button:hover {
        decorator: image( dat://0x06004c9f );
      }

      button#createcharacter {
        decorator: image( dat://0x06004CA3 );
        top: 185px;
        left: 215px;
        width: 254px;
        height: 113px;
        line-height: 113px;
        font-size: 21dp;
        font-weight: bold;
      }
      button#createcharacter:hover {
        decorator: image( dat://0x06004CA4 );
      }

      button#enter {
        decorator: image( dat://0x06004CB2 );
        top: 290px;
        left: 240px;
        width: 211px;
        height: 211px;
        line-height: 211px;
        font-size: 32dp;
        font-weight: bold;
      }
      button#enter:hover {
        decorator: image( dat://0x06004CB3 );
      }

      button#delete {
        top: 522px;
        left: 36px;
      }

      button#credits {
        top: 522px;
        left: 465px;
      }

      button#exit {
        top: 522px;
        left: 615px;
      }

      span#world {
        decorator: image( dat://0x06004D64 );
        display: inline-block;
        width: 193px;
        height: 110px;
        line-height: 110px;
        font-size: 22px;
        text-align: center;
        position: absolute;
        top: 44px;
        left: 20px;
      }
    </style>
	</head>
	<body id="b">
    <div id="wrapper">
      <span id="world">Coldeve</span>
      <button id="enter">ENTER</button>
      <button id="createcharacter">Create Character</button>
      <button id="credits">CREDITS</button>
      <button id="delete">DELETE</button>
      <button id="exit">EXIT</button>
      <div id="panel">
        <ul>
          <li>A Mage</li>
          <li>Sunnuj</li>
          <li>Do Things Bot</li>
        </ul>
      </div>
    </div>
